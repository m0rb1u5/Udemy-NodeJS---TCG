extends layouts/main-layout.pug

block styles 
    link(rel="stylesheet", href="/css/product.css")

block content
    main 
        if usrs.length > 0
            .grid 
                each user in usrs 
                    article.card.product-item 
                        header.card__header 
                            h1.product__title #{user.title}
                        .card__image 
                            img(src="data:image/jpeg;base64,", alt="A Book")
        else 
            h1 No Users